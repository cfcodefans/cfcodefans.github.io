{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "lib": [
            "dom",
            "dom.iterable",
            "esnext",
            "ESNext"
        ],
        "baseUrl": "./src",
        "module": "commonjs",
        "outDir": "dist",
        "target": "es2018",
        "isolatedModules": false,
        "jsx": "react",
        "noEmit": false
    },
    "include": [
        "**/*.ts",
        "**/*.tsx"
    ]
}